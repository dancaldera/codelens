{
  "name": "electron-overlay-example",
  "version": "1.0.0",
  "description": "Electron overlay window example",
  "main": "dist/src/main.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && electron .",
    "typescript-check": "tsc --noEmit",
    "dev": "concurrently \"npm run watch\" \"npm run electron-dev\"",
    "watch": "tsc -w",
    "electron-dev": "nodemon --watch dist --exec \"electron .\""
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.21",
    "ai": "^4.3.13",
    "dotenv": "^16.5.0",
    "electron": "^36.1.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.15.3",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3",
    "typescript": "^5.8.3"
  }
}
